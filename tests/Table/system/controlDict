FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     chemFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

//original 

endTime 20;
deltaT          1e-05;
maxDeltaT 1000;


//adjustTimeStep off;
adjustTimeStep  on;

writeControl    runTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

suppressSolverInfo yes;

